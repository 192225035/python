a=[]
n=int(input("enter number of element:"))
for i in range(1,n+1):
    b=int(input("enter the element:"))
    a.append(b)
even=[]
odd=[]
for j in a:
    if(j%2==0):
        even.append(j)
    else:
        odd.append(j)
print("even number are:",even)
print("odd number are:",odd)












































































































































































































































a=[]
n=int(input("enter number of element:"))
for i in range(1,n+1):
    b=int(input("enter the element:"))
    a.append(b)
even=[]
odd=[]
for j in a:
    if(j%2==0):
        even.append(j)
    else:
        odd.append(j)
print("even number are:",even)
print("odd number are:",odd)







































